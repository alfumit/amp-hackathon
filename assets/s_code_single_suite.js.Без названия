	if (/o|e|d/.test(instanceType)) {
		// Current instance is s.tl
		if (tmpOm.isSalePage) tmpOm.eVar59 = tmpOm.pageNameReal;
		tmpOm.instanceType = instanceType;
		tmpOm.debugTrace = s.apl(tmpOm.debugTrace, 's.tl', ' > ', 0);
